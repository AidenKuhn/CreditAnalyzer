{
  "name": "credit-analyzer",
  "private": true,
  "version": "1.0.0",
  "description": "Confidential credit evaluation system using Zama's FHE technology",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:contracts && tsc && vite build",
    "build:contracts": "cross-env TS_NODE_PROJECT=tsconfig.hardhat.json TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "build:frontend": "tsc && vite build",
    "clean": "rimraf ./artifacts ./cache ./coverage ./types ./coverage.json ./dist && npm run typechain",
    "compile": "cross-env TS_NODE_PROJECT=tsconfig.hardhat.json TS_NODE_TRANSPILE_ONLY=true hardhat compile",
    "coverage": "cross-env TS_NODE_PROJECT=tsconfig.hardhat.json SOLIDITY_COVERAGE=true hardhat coverage --solcoverjs ./.solcover.js --temp artifacts --testfiles \"test/**/*.ts\" && npm run typechain",
    "deploy": "cross-env TS_NODE_PROJECT=tsconfig.hardhat.json TS_NODE_TRANSPILE_ONLY=true hardhat run scripts/deploy.ts",
    "deploy:sepolia": "cross-env TS_NODE_PROJECT=tsconfig.hardhat.json TS_NODE_TRANSPILE_ONLY=true hardhat run scripts/deploy.ts --network sepolia",
    "lint": "npm run lint:sol && npm run lint:ts && npm run prettier:check",
    "lint:sol": "solhint --max-warnings 0 \"contracts/**/*.sol\"",
    "lint:ts": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "postcompile": "npm run typechain",
    "prettier:check": "prettier --check \"**/*.{js,json,md,sol,ts,tsx,yml}\"",
    "prettier:write": "prettier --write \"**/*.{js,json,md,sol,ts,tsx,yml}\"",
    "preview": "vite preview",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.hardhat.json hardhat test",
    "test:coverage": "npm run coverage",
    "typechain": "cross-env TS_NODE_PROJECT=tsconfig.hardhat.json TS_NODE_TRANSPILE_ONLY=true hardhat typechain"
  },
  "dependencies": {
    "@fhevm/solidity": "0.7.0",
    "@openzeppelin/contracts": "5.1.0",
    "@openzeppelin/contracts-upgradeable": "5.1.0",
    "ethers": "6.15.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@fhevm/hardhat-plugin": "0.0.1-3",
    "@nomicfoundation/hardhat-chai-matchers": "2.0.8",
    "@nomicfoundation/hardhat-ethers": "3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "1.0.12",
    "@nomicfoundation/hardhat-verify": "2.0.14",
    "@typechain/ethers-v6": "0.5.1",
    "@typechain/hardhat": "9.1.0",
    "@types/chai": "4.3.11",
    "@types/mocha": "10.0.10",
    "@types/node": "20.10.4",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "@vitejs/plugin-react": "4.2.1",
    "chai": "4.5.0",
    "chai-as-promised": "8.0.1",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.6",
    "hardhat": "2.24.3",
    "hardhat-gas-reporter": "2.3.0",
    "mocha": "11.5.0",
    "prettier": "3.5.3",
    "prettier-plugin-solidity": "1.2.0",
    "rimraf": "6.0.1",
    "solhint": "5.1.0",
    "solhint-plugin-prettier": "0.1.0",
    "solidity-coverage": "0.8.16",
    "ts-generator": "0.1.1",
    "ts-node": "10.9.2",
    "typechain": "8.3.2",
    "typescript": "5.2.2",
    "vite": "5.2.0"
  },
  "keywords": [
    "fhe",
    "privacy",
    "credit-scoring",
    "blockchain",
    "encryption",
    "zama",
    "ethereum",
    "react",
    "typescript",
    "vite"
  ],
  "author": "Credit Analyzer",
  "license": "MIT"
}
